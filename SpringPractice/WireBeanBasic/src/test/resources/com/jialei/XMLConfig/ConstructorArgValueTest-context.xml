<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="blankDisc" class="com.jialei.XMLConfig.BlankDiscXMLConfig">
        <constructor-arg name="title" value="Sgt. Pepper's Lonely Hearts Club Band"/>
        <constructor-arg name="artist" value="The Beatles"/>
    </bean>

    <bean id="cdPlayer" class="com.jialei.XMLConfig.CDPlayerXMLConfig">
        <constructor-arg ref="blankDisc"/>
    </bean>

</beans>